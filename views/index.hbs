<h1>Availible maps</h1>
{{#eachInMap grouped}}
<div class="realm-collection">
    <h2>{{key}}</h2>
    {{#each value}}
    <a href="{{item.title}}" class="realm">
        <h3>{{item.title}}</h3>
        {{> minimap array=item.minimap.array colors=../../colors}}
    </a>
    {{/each}}
</div>
{{/eachInMap}}